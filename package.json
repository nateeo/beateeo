{
  "name": "discord-bot",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "bufferutil": "^4.0.0",
    "discord.js": "^11.3.2",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^2.0.0",
    "electron-updater": "^3.1.5",
    "erlpack": "hammerandchisel/erlpack",
    "libsodium-wrappers": "^0.7.3",
    "node-notifier": "^8.0.1",
    "node-opus": "^0.3.0",
    "react": "^16.4.0",
    "react-beautiful-dnd": "^7.1.3",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "sodium": "^2.0.3",
    "styled-components": "^3.3.2",
    "ytdl-core": "^0.20.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "prod": "CSC_IDENTITY_AUTO_DISCOVERY=false build --c.extraMetadata.main=build/electron.js",
    "release": "build --c.extraMetadata.main=build/electron.js --publish=always",
    "postinstall": "yarn run electron-rebuild",
    "preprod": "yarn build",
    "prerelease": "yarn build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "precommit": "pretty-quick --staged",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.nateeo.beateeo",
    "productName": "Beateeo",
    "mac": {
      "identity": null
    }
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-rebuild": "^1.8.6",
    "husky": "^0.14.3",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "wait-on": "^2.1.0"
  }
}
